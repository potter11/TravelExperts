<%- include('partials/header') %> 
<%- include('partials/navbar') %>

<div class="container-fluid" style="padding: 0;">
  <div class="jumbotron jumbotron-fluid" style="background-image: url('https://images.unsplash.com/photo-1522071901873-411886a10004?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60'); background-size: cover; color: honeydew; padding: 100px 0;">
    <div class="container-fluid">
      <h1 class="display-4">Agents</h1>
      <p class="lead">Contact Us</p>
    </div>
  </div>
  <div class='table-responsive'>
    <table class="table table-borderless bg-light" style="margin: auto; margin-top: 80px; border-radius: 10px;">
      <caption>List of Agents</caption>
      <thead>
        <tr>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Phone Number</th>
        </tr>
      </thead>
      
      <tbody>
          <% agents.forEach(function (agents)  { %>
        <tr>
          <td><%=agents.AgtFirstName%></td>
          <td><%=agents.AgtLastName%></td>
          <td><%=agents.AgtBusPhone%></td>
        </tr>
      </tbody>
      <% }) %>
    </table>
  </div>

</div>


<%- include('partials/footer') %>
